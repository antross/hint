    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.12.0",
    "ava": "^2.4.0",
    "browserslist": "^4.8.5",
    "cpx": "^1.5.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.18.2",
