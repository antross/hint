<details class="category">
    <summary class="category__summary">
        <span class="category__name">
            <%= name %>
        </span>
        <span class="category__status">
            PASSED: <%= passed %>/<%= hints.length %>
        </span>
    </summary>
    <div class="category__results">
        <% hints.forEach(function (hint) { %>
            <%- include('hint', hint); %>
        <% }); %>
    </div>
</details>
